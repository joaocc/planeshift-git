<widget_description>

<widget name="PartListToolbox" factory="EEditParticleListToolbox" 
        visible="yes" savepositions="yes" movable="yes" resizable="no" resizetoscreen="no" >
        
    <frame x="0" y="490" width="560" height="230" border="yes" justtitle="yes" />
    <title resource="TitleBar" text="Particle Systems" align="left" />
    <font name="/planeshift/data/ttf/LiberationSans-Regular.ttf" r="255" g="255" b="255" size="9"  sr="0" sg="0" sb="0" shadow="yes" />   
    <bgimage resource="BackTall" alpha="75" />

    <widget name="part_list" factory="pawsListBox">	
        <frame x="5" y="5" width="200" height="190" border="no"/>
        <bgimage resource="Shaded" alpha="100" />
        <font name="/planeshift/data/ttf/LiberationSans-Regular.ttf" r="255" g="255" b="255" size="9"  sr="0" sg="0" sb="0" shadow="yes" />   
        <highlight resource="ListSelect" alpha="100" />
        <columns height="20" headings="no">
            <column width="180">            
                <widget name="PartSys" factory="pawsTextBox">
                </widget>
            </column>                                               
        </columns>
	</widget>

    <widget name="edit_list" factory="pawsListBox">	
        <frame x="210" y="5" width="140" height="190" border="no"/>
        <bgimage resource="Shaded" alpha="100" />
        <font name="/planeshift/data/ttf/LiberationSans-Regular.ttf" r="255" g="255" b="255" size="9"  sr="0" sg="0" sb="0" shadow="yes" />   
        <highlight resource="ListSelect" alpha="100" />
        <columns height="20" headings="no">
            <column width="40">            
                <widget name="Type" factory="pawsTextBox">
                </widget>
            </column>                                               
            <column width="140">            
                <widget name="Description" factory="pawsTextBox">
                </widget>
            </column>                                               
        </columns>
	</widget>

    <widget name="parm_list" factory="pawsListBox">	
        <frame x="355" y="5" width="200" height="120" border="no"/>
        <bgimage resource="Shaded" alpha="100" />
        <font name="/planeshift/data/ttf/LiberationSans-Regular.ttf" r="255" g="255" b="255" size="9"  sr="0" sg="0" sb="0" shadow="yes" />   
        <highlight resource="ListSelect" alpha="100" />
        <columns height="20" headings="no">
            <column width="80">            
                <widget name="Name" factory="pawsTextBox">
                </widget>
            </column>                                               
            <column width="25">            
                <widget name="Type" factory="pawsTextBox">
                </widget>
            </column>                                               
            <column width="75">            
                <widget name="Value" factory="pawsTextBox">
                </widget>
            </column>                                               
        </columns>
	</widget>

    <widget name="value_bool" factory="pawsCheckBox">
        <frame x="355" y="130" width="200" height="25" border="yes" />
        <font name="/planeshift/data/ttf/LiberationSans-Regular.ttf" r="255" g="255" b="255" size="9"  sr="0" sg="0" sb="0" shadow="yes" />
	<checkbox off="RadioOff" on="RadioOn" />
	<text string="..." />
        <bgimage resource="Shaded" alpha="75" />
    </widget>

    <widget name="value_choices" factory="pawsComboBox">
        <frame x="355" y="130" width="200" height="25" border="yes" />
        <bgimage resource="Shaded" alpha="100" />
	<listbox rows="5" height="25" useScrollBar="false"/>
    </widget>

    <widget name="value_num" factory="pawsSpinBox">
        <frame x="355" y="130" width="200" height="25" border="no" />
        <font name="/planeshift/data/ttf/LiberationSans-Regular.ttf" r="255" g="255" b="255" size="9"  sr="0" sg="0" sb="0" shadow="yes" />
        <bgimage resource="Shaded" alpha="75" />
        <number default="0.0" min="1" max="300" inc="1" position="left"/>
    </widget>

    <widget name="value2_num" factory="pawsSpinBox">
        <frame x="355" y="160" width="200" height="25" border="no" />
        <font name="/planeshift/data/ttf/LiberationSans-Regular.ttf" r="255" g="255" b="255" size="9"  sr="0" sg="0" sb="0" shadow="yes" />
        <bgimage resource="Shaded" alpha="75" />
        <number default="0.0" min="1" max="300" inc="1" position="left"/>
    </widget>

    <widget name="value3_num" factory="pawsSpinBox">
        <frame x="355" y="190" width="200" height="25" border="no" />
        <font name="/planeshift/data/ttf/LiberationSans-Regular.ttf" r="255" g="255" b="255" size="9"  sr="0" sg="0" sb="0" shadow="yes" />
        <bgimage resource="Shaded" alpha="75" />
        <number default="0.0" min="1" max="300" inc="1" position="left"/>
    </widget>

    <widget name="save_part_button" factory="pawsButton">
        <frame x="215" y="200" width="95" height="25" border="no" />
        <font name="/planeshift/data/ttf/LiberationSans-Regular.ttf" r="255" g="255" b="255" size="7"  sr="0" sg="0" sb="0" shadow="yes" />   
        <buttonup resource="ButtonUp"/>
        <buttondown resource="ButtonDown" />
        <label text="Save"/>
    </widget>
    <widget name="load_part_button" factory="pawsButton">
        <frame x="110" y="200" width="95" height="25" border="no" />
        <font name="/planeshift/data/ttf/LiberationSans-Regular.ttf" r="255" g="255" b="255" size="7"  sr="0" sg="0" sb="0" shadow="yes" />   
        <buttonup resource="ButtonUp"/>
        <buttondown resource="ButtonDown" />
        <label text="Open Particles"/>
    </widget>
    <widget name="refresh_button" factory="pawsButton">
        <frame x="5" y="200" width="95" height="25" border="no" />
        <font name="/planeshift/data/ttf/LiberationSans-Regular.ttf" r="255" g="255" b="255" size="7"  sr="0" sg="0" sb="0" shadow="yes" />   
        <buttonup resource="ButtonUp"/>
        <buttondown resource="ButtonDown" />
        <label text="Refresh"/>
    </widget>
</widget>

</widget_description>
