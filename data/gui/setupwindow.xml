<widget_description>

<widget name="setup" factory="pawsSetupWindow" visible="yes" savepositions="no" movable="no" resizable="no" resizetoscreen="yes">

    <bgimage resource="SetupBackground" alpha="255" fade="false"/>

    <frame x="0" y="0" width="850" height="600" border="no"/>
    <title resource="Blue Title" text="Planeshift Setup" align="left"/>

    <widget name="res" factory="pawsTextBox">
        <frame x="50" y="30" width="250" height="30"/>
        <font name="/planeshift/data/ttf/LiberationSans-Regular.ttf" r="127" g="164" b="14" size="12" sr="0" sg="0" sb="0" shadow="yes" resizetoscreen="yes"/>
        <text string="Screen Resolution"/>
    </widget>

    <widget name="resbox" factory="pawsRadioButtonGroup">
        <frame x="60" y="20" width="150" height="135" border="no"/>
 		<font name="/planeshift/data/ttf/LiberationSans-Regular.ttf" r="255" g="255" b="255" size="12" resizetoscreen="yes"/>
        <radio on="radioon" off="radiooff" size="12"/>

        <widget name="800x600" factory="pawsRadioButton" id="1">
            <frame x="0" y="35" width="300" height="30"/>
            <text string="800x600" position="right"/>
        </widget>

        <widget name="1024x768" factory="pawsRadioButton" id="2">
            <frame x="0" y="65" width="300" height="30"/>
            <text string="1024x768" position="right"/>
        </widget>

        <widget name="rescustom" factory="pawsRadioButton" id="3">
            <frame x="0" y="95" width="300" height="30"/>
            <text string="Other:" position="right"/>
        </widget>
    </widget>

    <widget name="screenwidth" factory="pawsEditTextBox">
		<frame x="85" y="142" width="60" height="30" border="yes"/>
 		<font name="/planeshift/data/ttf/LiberationSans-Regular.ttf" r="255" g="255" b="255" size="12" resizetoscreen="yes"/>
	</widget>
		
    <widget name="x" factory="pawsTextBox">
		<frame x="147" y="147" width="30" height="30" border="no"/>
 		<font name="/planeshift/data/ttf/LiberationSans-Regular.ttf" r="255" g="255" b="255" size="12" resizetoscreen="yes"/>
 		<text string="x"/>
	</widget>
	
    <widget name="screenheight" factory="pawsEditTextBox">
		<frame x="160" y="142" width="60" height="30" border="yes"/>
 		<font name="/planeshift/data/ttf/LiberationSans-Regular.ttf" r="255" g="255" b="255" size="12" resizetoscreen="yes"/>
	</widget>
	
    <widget name="fullscreen" factory="pawsCheckBox">
        <frame x="60" y="175" width="70" height="32"/>
        <checkbox on="radioon" off="radiooff" size="12"/>
        <text string="" position="right"/>
    </widget>

    <widget name="fs" factory="pawsTextBox">
		<frame x="85" y="182" width="130" height="30" border="no"/>
 		<font name="/planeshift/data/ttf/LiberationSans-Regular.ttf" r="255" g="255" b="255" size="12" resizetoscreen="yes"/>
 		<text string="Fullscreen Mode"/>
	</widget>


    <widget name="screendepth" factory="pawsTextBox">
        <frame x="50" y="205" width="200" height="32"/>
        <font name="/planeshift/data/ttf/LiberationSans-Regular.ttf" r="127" g="164" b="14" size="12" sr="0" sg="0" sb="0" shadow="yes" resizetoscreen="yes"/>
        <text string="Color Depth"/>
    </widget>

    <widget name="depthbox" factory="pawsRadioButtonGroup">
        <frame x="60" y="195" width="200" height="135" border="no"/>
 		<font name="/planeshift/data/ttf/LiberationSans-Regular.ttf" r="255" g="255" b="255" size="12" resizetoscreen="yes"/>
        <radio on="radioon" off="radiooff" size="12"/>

        <widget name="15" factory="pawsRadioButton" id="1">
            <frame x="0" y="25" width="150" height="30"/>
            <text string="15 bit" position="right"/>
        </widget>

        <widget name="16" factory="pawsRadioButton" id="2">
            <frame x="0" y="55" width="150" height="30"/>
            <text string="16 bit" position="right"/>
        </widget>

        <widget name="32" factory="pawsRadioButton" id="3">
            <frame x="0" y="85" width="150" height="30"/>
            <text string="32 bit" position="right"/>
        </widget>
    </widget>

    <widget name="soundoptions" factory="pawsTextBox">
        <frame x="50" y="345" width="300" height="30"/>
        <font name="/planeshift/data/ttf/LiberationSans-Regular.ttf" r="127" g="164" b="14" size="12" sr="0" sg="0" sb="0" shadow="yes" resizetoscreen="yes"/>
        <text string="Sound"/>
    </widget>

    <widget name="sound" factory="pawsCheckBox">
        <frame x="60" y="365" width="300" height="32"/>
        <checkbox on="radioon" off="radiooff" size="12"/>
        <text string="Enable Sound" position="right"/>
        <font name="/planeshift/data/ttf/LiberationSans-Regular.ttf" r="255" g="255" b="255" size="12" resizetoscreen="yes"/>
    </widget>
	
    <widget name="fontscalefactor" factory="pawsTextBox">
        <frame x="50" y="410" width="200" height="32"/>
        <font name="/planeshift/data/ttf/LiberationSans-Regular.ttf" r="127" g="164" b="14" size="12" sr="0" sg="0" sb="0" shadow="yes" resizetoscreen="yes"/>
        <text string="Font Scaling"/>
    </widget>

    <widget name="fontscale" factory="pawsScrollBar" direction="horizontal" >
        <frame x="50" y="430" width="130" height="15" border="no" />
    </widget>

    <widget name="fontscalepct" factory="pawsTextBox">
        <frame x="190" y="428" width="50" height="50"/>
        <font name="/planeshift/data/ttf/LiberationSans-Regular.ttf" r="255" g="255" b="255" size="12" resizetoscreen="yes"/>
        <text string="100%"/>
    </widget>


    <!-- Advanced section -->

     <widget name="advanced" factory="pawsTextBox">
        <frame x="280" y="20" width="300" height="50"/>
        <font name="/planeshift/data/ttf/LiberationSans-Regular.ttf" r="127" g="164" b="14" size="12" sr="0" sg="0" sb="0" shadow="yes" resizetoscreen="yes"/>
        <text string="Advanced options"/>
    </widget>

    <!-- VBO -->
    <widget name="vbolbl" factory="pawsTextBox">
        <frame x="290" y="40" width="300" height="25"/>
        <font name="/planeshift/data/ttf/LiberationSans-Regular.ttf" r="255" g="255" b="255" size="12" sr="0" sg="0" sb="0" shadow="yes" resizetoscreen="yes"/>
        <text string="Vertex Buffer Object"/>
    </widget>

    <widget name="vbobox" factory="pawsRadioButtonGroup">
        <frame x="290" y="50" width="200" height="110" border="no"/>
 		<font name="/planeshift/data/ttf/LiberationSans-Regular.ttf" r="255" g="255" b="255" size="12" resizetoscreen="yes"/>
        <radio on="radioon" off="radiooff" size="12"/>

        <widget name="def" factory="pawsRadioButton" id="0">
            <frame x="10" y="10" width="300" height="30"/>
            <text string="Default" position="right"/>
        </widget>

        <widget name="on" factory="pawsRadioButton" id="1">
            <frame x="10" y="40" width="300" height="30"/>
            <text string="On" position="right"/>
        </widget>

        <widget name="off" factory="pawsRadioButton" id="2">
            <frame x="10" y="70" width="300" height="30"/>
            <text string="Off" position="right"/>
        </widget>
    </widget>

    <!-- Stencil Threshold -->
    <widget name="stlbl" factory="pawsTextBox">
        <frame x="290" y="150" width="150" height="30"/>
        <font name="/planeshift/data/ttf/LiberationSans-Regular.ttf" r="255" g="255" b="255" size="12" sr="0" sg="0" sb="0" shadow="yes" resizetoscreen="yes"/>
        <text string="Stencil Threshold:"/>
    </widget>

    <widget name="stencil" factory="pawsEditTextBox">
        <frame x="440" y="146" width="40" height="30" border="yes"/>
        <font name="/planeshift/data/ttf/LiberationSans-Regular.ttf" r="255" g="255" b="255" size="12" sr="0" sg="0" sb="0" shadow="yes" resizetoscreen="yes"/>
        <text string="50"/>
    </widget>

    <!-- All maps at once -->
    <widget name="allmaps" factory="pawsCheckBox">
        <frame x="290" y="175" width="200" height="32"/>
        <checkbox on="radioon" off="radiooff" size="12"/>
        <text string="Load all maps" position="right"/>
        <font name="/planeshift/data/ttf/LiberationSans-Regular.ttf" r="255" g="255" b="255" size="12" resizetoscreen="yes"/>
    </widget>
	
    <!-- Keep maps -->
    <widget name="keepmaps" factory="pawsCheckBox">
        <frame x="290" y="200" width="200" height="32"/>
        <checkbox on="radioon" off="radiooff" size="12"/>
        <text string="Keep maps loaded" position="right"/>
        <font name="/planeshift/data/ttf/LiberationSans-Regular.ttf" r="255" g="255" b="255" size="12" resizetoscreen="yes"/>
    </widget>

    <!-- Preload Models -->
    <widget name="preloadmodels" factory="pawsCheckBox">
        <frame x="290" y="225" width="200" height="32"/>
        <checkbox on="radioon" off="radiooff" size="12"/>
        <text string="Preload Models" position="right"/>
        <font name="/planeshift/data/ttf/LiberationSans-Regular.ttf" r="255" g="255" b="255" size="12" resizetoscreen="yes"/>
    </widget>

    <!-- Skins button -->
    <widget name="Skins" factory="pawsButton">
        <frame x="290" y="270" width="130" height="30" border="no"/>
        <font name="/planeshift/data/ttf/LiberationSans-Regular.ttf" r="255" g="255" b="255" size="12" resizetoscreen="yes"/>
         <buttonup resource="SetupButtonDown"/>
         <buttondown resource="SetupButtonDown"/>
		<label text="Skins"/>
    </widget>

    <!-- Quality section -->

     <widget name="qual" factory="pawsTextBox">
        <frame x="280" y="320" width="300" height="25" />
        <font name="/planeshift/data/ttf/LiberationSans-Regular.ttf" r="127" g="164" b="14" size="12" sr="0" sg="0" sb="0" shadow="yes" resizetoscreen="yes"/>
        <text string="Quality / Speed options"/>
    </widget>

    <!-- MultiSampling -->
    <widget name="msl" factory="pawsTextBox">
        <frame x="290" y="350" width="210" height="30"/>
        <font name="/planeshift/data/ttf/LiberationSans-Regular.ttf" r="255" g="255" b="255" size="12" resizetoscreen="yes"/>
        <text string="Multisampling (Antialiasing): "/>
    </widget>
    <widget name="multi" factory="pawsScrollBar" direction="horizontal" >
        <frame x="500" y="356" width="120" height="15" border="no" />
    </widget>

    <widget name="ms" factory="pawsTextBox">
        <frame x="620" y="350" width="40" height="30"/>
        <font name="/planeshift/data/ttf/LiberationSans-Regular.ttf" r="255" g="255" b="255" size="12" resizetoscreen="yes"/>
        <text string="0"/>
     </widget>

    <!-- Anisotropy filter -->
    <widget name="atl" factory="pawsTextBox">
        <frame x="290" y="380" width="210" height="30"/>
        <font name="/planeshift/data/ttf/LiberationSans-Regular.ttf" r="255" g="255" b="255" size="12" resizetoscreen="yes"/>
        <text string="Anisotropy filtering: "/>
    </widget>
    <widget name="ani" factory="pawsScrollBar" direction="horizontal" >
        <frame x="500" y="386" width="120" height="15" border="no" />
    </widget>

    <widget name="at" factory="pawsTextBox">
        <frame x="620" y="380" width="40" height="50"/>
        <font name="/planeshift/data/ttf/LiberationSans-Regular.ttf" r="255" g="255" b="255" size="12" resizetoscreen="yes"/>
        <text string="0.0"/>
    </widget>

    <!-- Quality or speed filter -->
    <widget name="ms_quality" factory="pawsCheckBox">
        <frame x="290" y="405" width="350" height="32"/>
        <checkbox on="radioon" off="radiooff" size="12"/>
        <font name="/planeshift/data/ttf/LiberationSans-Regular.ttf" r="255" g="255" b="255" size="12" resizetoscreen="yes"/>
        <text string="Multisample for quality "/>
    </widget>

    <!-- Texture downsampling-->
    <widget name="texl" factory="pawsTextBox">
        <frame x="290" y="445" width="210" height="30"/>
        <font name="/planeshift/data/ttf/LiberationSans-Regular.ttf" r="255" g="255" b="255" size="12" resizetoscreen="yes"/>
        <text string="Texture downsampling: "/>
    </widget>
    <widget name="texdown" factory="pawsScrollBar" direction="horizontal" >
        <frame x="500" y="451" width="120" height="15" border="no" />
    </widget>

    <widget name="texdownc" factory="pawsTextBox">
        <frame x="620" y="445" width="40" height="50"/>
        <font name="/planeshift/data/ttf/LiberationSans-Regular.ttf" r="255" g="255" b="255" size="12" resizetoscreen="yes"/>
        <text string="1/1"/>
    </widget>

    <!-- OK button -->
    <widget name="OKButton" factory="pawsButton">
        <frame x="210" y="520" width="130" height="30" border="no"/>
        <font name="/planeshift/data/ttf/LiberationSans-Regular.ttf" r="255" g="255" b="255" size="12" resizetoscreen="yes"/>
         <buttonup resource="SetupButtonDown"/>
         <buttondown resource="SetupButtonDown"/>
		<label text="OK"/>
    </widget>

       <!-- Launch button -->
    <widget name="LaunchButton" factory="pawsButton">
        <frame x="350" y="520" width="130" height="30" border="no"/>
        <font name="/planeshift/data/ttf/LiberationSans-Regular.ttf" r="255" g="255" b="255" size="12" resizetoscreen="yes"/>
         <buttonup resource="SetupButtonDown"/>
         <buttondown resource="SetupButtonDown"/>
		<label text="Launch"/>
    </widget>

   <!-- Cancel button -->
    <widget name="CancelButton" factory="pawsButton" toggle="no" shadows="yes">
        <frame x="490" y="520" width="130" height="30" border="no"/>
       	<font name="/planeshift/data/ttf/LiberationSans-Regular.ttf" r="255" g="255" b="255" size="12" resizetoscreen="yes"/>
         <buttonup resource="SetupButtonDown"/>
         <buttondown resource="SetupButtonDown"/>
		<label text="Cancel"/>
    </widget>

</widget>

</widget_description>
