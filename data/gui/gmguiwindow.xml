					<font name="/planeshift/data/ttf/LiberationSans-Regular.ttf" r="255" g="255" b="255" size="10" />
                </widget>        
                <widget name="BanDays" factory="pawsEditTextBox">    
                    <frame x="568" y="309" width="20" height="20" border="no"/> 
                    <text string="0" horizAdjust="CENTRE" />    
					<font name="/planeshift/data/ttf/LiberationSans-Regular.ttf" r="255" g="255" b="255" size="10" />
                </widget>
					
                <widget name="BanMinlbl" factory="pawsTextBox" style="label">    
