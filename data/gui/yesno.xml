<widget_description>

<widget name="YesNoWindow" factory="pawsYesNoBox" 
        visible="no" savepositions="no" movable="yes"
        resizable="no" style="New Standard GUI" >

        
    <frame x="0" y="0" width="500" height="250" border="no" />
    
        <widget name="Message Box" factory="pawsMultiLineTextBox">
            <frame x="100" y="40" width="300" height="150" border="no" />
        </widget>

        <widget name="YesButton" factory="pawsButton" id="-10" key="Y">
            <frame x="140" y="185" width="100" height="40" />
            <label text="Yes" />
        </widget>

        <widget name="NoButton" factory="pawsButton" id="-20" key="N">
            <frame x="260" y="185" width="100" height="40" />
            <label text="No" />
        </widget>

</widget>

</widget_description>
