<widget_description>
  <pawsYesNoBox name="YesNoWindow" visible="no" savepositions="no"
                movable="yes" resizable="no" style="QuestionBox">
    <frame x="0" y="0" width="500" height="250" border="no"/>
    
    <pawsMultiLineTextBox name="Message Box">
      <frame x="100" y="40" width="300" height="150" border="no"/>
    </pawsMultiLineTextBox>

    <pawsButton name="YesButton" id="-10" key="Y">
      <frame x="140" y="195" width="100" height="30"/>
      <label text="Yes"/>
    </pawsButton>

    <pawsButton name="NoButton" id="-20" key="N">
      <frame x="260" y="195" width="100" height="30"/>
      <label text="No"/>
    </pawsButton>
  </pawsYesNoBox>
</widget_description>
